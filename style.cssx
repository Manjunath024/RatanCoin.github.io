/* Animation definitions */
@keyframes bounceIn {
    0% {
        transform: scale(0);
        opacity: 0;
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
        opacity: 1;
    }
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes zoomIn {
    from {
        transform: scale(0);
        opacity: 0;
    }

    to {
        transform: scale(1);
        opacity: 1;
    }
}

.roadmap1 {
    font-family: 'Poppins', sans-serif;
    margin: 0;
    padding: 0;
    background: #e9f5dc;
    color: #2e3d49;
}

/* Roadmap Container */
.roadmap {
    text-align: center;
    padding: 2rem;
}

.roadmap-title {
    font-size: 2.8rem;
    font-weight: bold;
    color: #354d29;
    margin-bottom: 2rem;
    animation: fadeIn 2s ease-in;
}

/* Timeline Layout */
.timeline {
    position: relative;
    max-width: 900px;
    margin: 0 auto;
}

.phase {
    display: flex;
    align-items: center;
    margin: 2.5rem 0;
    position: relative;
    opacity: 0;
    transform: translateY(50px);
    animation: fadeUp 1.5s ease-in forwards;
}

.phase:nth-child(odd) {
    flex-direction: row-reverse;
}

.phase-circle {
    width: 50px;
    height: 50px;
    background: #82a666;
    border: 4px solid #354d29;
    border-radius: 50%;
    flex-shrink: 0;
    animation: pulse 2s infinite;
    box-shadow: 0 0 15px #354d29;
}

.phase-content {
    background: #f8f3eb;
    border-radius: 10px;
    padding: 1.5rem;
    margin: 0 1.5rem;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.15);
    text-align: left;
    border-left: 4px solid #82a666;
}

.phase-content h2 {
    font-size: 1.8rem;
    font-weight: bold;
    color: #354d29;
    margin-bottom: 0.5rem;
}

.phase-content p {
    font-size: 1rem;
    line-height: 1.6;
    color: #6d6d6d;
}

/* Animations */
@keyframes fadeUp {
    0% {
        opacity: 0;
        transform: translateY(50px);
    }

    100% {
        opacity: 1;
        transform: translateY(0);
    }
}

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes pulse {

    0%,
    100% {
        transform: scale(1);
        box-shadow: 0 0 15px #354d29;
    }

    50% {
        transform: scale(1.2);
        box-shadow: 0 0 25px #82a666;
    }
}

/* Responsive Design */
@media (max-width: 768px) {
    .timeline {
        max-width: 100%;
    }

    .phase {
        flex-direction: column !important;
    }

    .phase-content {
        margin: 1rem 0;
        text-align: center;
    }
}

.containerfaq {
    font-family: 'Roboto', sans-serif;
    background: linear-gradient(to bottom, #f7f9f7, #d9ead3);
    margin: 0;
    padding: 0;
    color: #333;
}

.faq-section {
    max-width: 800px;
    margin: 50px auto;
    padding: 20px;
}

.faq-title {
    text-align: center;
    font-family: 'Poppins', sans-serif;
    font-size: 2rem;
    font-weight: 600;
    color: #2d472c;
    margin-bottom: 20px;
}

.faq-container {
    display: flex;
    flex-direction: column;
    gap: 20px;
}

.faq-item {
    background: #ffffff;
    border: 2px solid #a4c493;
    border-radius: 10px;
    box-shadow: 0px 5px 15px rgba(0, 0, 0, 0.1);
    padding: 20px;
    transition: transform 0.3s, box-shadow 0.3s;
}

.faq-item:hover {
    transform: scale(1.02);
    box-shadow: 0px 8px 20px rgba(0, 0, 0, 0.2);
}

.faq-question {
    font-family: 'Poppins', sans-serif;
    font-size: 1.2rem;
    font-weight: 600;
    color: #355e3b;
    margin: 0 0 10px 0;
}

.faq-answer {
