/* Buy Container */
.buy-container {

    background: #1a1a27;
    padding: 30px;
    max-width: 900px;
    text-align: center;
    border-radius: 15px;
    position: relative;
    border: 2px solid #3a3a5a;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.6);
    animation: fadeIn 1.5s ease-in-out;
    margin: 26px;
}

/* Heading */
.buy-container h1 {
    font-size: 1.8rem;
    margin-bottom: 20px;
    color: #4fc3f7;
    font-weight: 600;
}

/* Steps Section */
.steps p {
    font-size: 1rem;
    margin: 10px 0;
    color: #b0bec5;
}

/* Address Box */
.address-box {
    background: rgba(48, 63, 159, 0.1);
    padding: 15px;
    border-radius: 12px;
    margin: 20px 0;
    border: 2px solid #4fc3f7;
    box-shadow: 0 2px 10px rgba(79, 195, 247, 0.2);
    transition: transform 0.3s ease-in-out;
}

.address-box:hover {
    transform: scale(1.02);
}

/* Wallet Address Button */
.address-button {
    font-size: 0.7rem;
    font-family: 'Poppins', sans-serif;
    font-weight: 600;
    color: #fff;
    background: linear-gradient(135deg, #4fc3f7, #0288d1);
    padding: 12px 20px;
    border-radius: 8px;
    cursor: pointer;
    border: none;
    box-shadow: 0 4px 15px rgba(79, 195, 247, 0.3);
    transition: transform 0.3s, box-shadow 0.3s;
}

.address-button:hover {
    transform: scale(1.05);
    box-shadow: 0 8px 20px rgba(79, 195, 247, 0.5);
}

/* Info Text */
.info {
    font-size: 0.9rem;
    margin-top: 10px;
    color: #b0bec5;
    font-style: italic;
}

/* Animations */
@keyframes fadeIn {
    from {
        opacity: 0;
        transform: translateY(20px);
    }

    to {
        opacity: 1;
        transform: translateY(0);
    }
}


